name: fastai_example

docker_env: 
  image: mlflow-fastai

entry_points:
  main:
    parameters:
      lr: {type: float, default: 0.01}
      epochs: {type: int, default: 5}
    command: |
        python 
          image: ./fastai_example/train.py \
            --lr {lr} \
            --epochs {epochs}
