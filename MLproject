name: fastai_example

docker_env: 
  image: seebaktec/mlflow-fastai
  environment: [["AWS_ROLE_ARN", "arn:aws:iam::630835944865:role/AdminFullAccess"], "AWS_ACCESS_KEY_ID", "AWS_SECRET_ACCESS_KEY"]

entry_points:
  main:
    parameters:
      lr: {type: float, default: 0.01}
      epochs: {type: int, default: 5}
    command: |
        python ./fastai_example/train.py \
            --lr {lr} \
            --epochs {epochs}
