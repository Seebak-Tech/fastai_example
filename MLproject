name: fastai_example

docker_env: 
  image: seebaktec/mlflow-fastai
# volumes: ["$HOME/.aws:/root/.aws"]
  environment: [["MLFLOW_PROJECT", "/mlflow/projects/code"]] 

entry_points:
  main:
    parameters:
      lr: {type: float, default: 0.01}
      epochs: {type: int, default: 5}
    command: |
        python ./fastai_example/train.py \
            --lr {lr} \
            --epochs {epochs}
